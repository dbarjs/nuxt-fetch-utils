{
  "name": "nuxt-fetch-utils",
  "type": "module",
  "version": "1.0.0",
  "packageManager": "pnpm@10.14.0",
  "description": "A Nuxt module for improving data fetching and management in Nuxt applications.",
  "author": "Eduardo Barros <dbarjs@jsbx.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/dbarjs",
  "homepage": "https://github.com/dbarjs/nuxt-fetch-utils#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dbarjs/nuxt-fetch-utils.git"
  },
  "keywords": [
    "nuxt-fetch-utils",
    "nuxt",
    "vue3",
    "tailwindcss",
    "component library",
    "vue",
    "vuejs",
    "vue.js",
    "typescript",
    "vue-components",
    "fetch",
    "ofetch",
    "reka-ui"
  ],
  "exports": {
    ".": {
      "types": "./dist/types.d.mts",
      "import": "./dist/module.mjs"
    }
  },
  "main": "./dist/module.mjs",
  "typesVersions": {
    "*": {
      ".": [
        "./dist/types.d.mts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "dev": "npm run dev:prepare && nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "release": "npm run lint && npm run test && npm run prepack && changelogen --release && npm publish && git push --follow-tags",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:types": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit"
  },
  "peerDependencies": {
    "typescript": "^5.6.3",
    "vue-router": "^4.5.0",
    "yup": "^1.6.0",
    "zod": "^3.24.0 || ^4.0.0"
  },
  "peerDependenciesMeta": {
    "zod": {
      "optional": true
    }
  },
  "dependencies": {
    "@nuxt/kit": "^4.0.3",
    "@vueuse/core": "^13.6.0",
    "ohash": "^2.0.11",
    "pathe": "^2.0.3",
    "reka-ui": "^2.0.0",
    "scule": "^1.3.0",
    "vue-component-type-helpers": "^3.0.5"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^5.2.1",
    "@nuxt/devtools": "^2.6.2",
    "@nuxt/eslint-config": "^1.8.0",
    "@nuxt/module-builder": "^1.0.2",
    "@nuxt/schema": "^4.0.3",
    "@nuxt/test-utils": "^3.19.2",
    "@types/node": "latest",
    "changelogen": "^0.6.2",
    "eslint": "^9.33.0",
    "nuxt": "^4.0.3",
    "typescript": "~5.9.2",
    "vitest": "^3.2.4",
    "vue-tsc": "^3.0.5"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "better-sqlite3",
      "puppeteer",
      "sharp"
    ],
    "ignoredBuiltDependencies": [
      "@parcel/watcher",
      "esbuild",
      "vue-demi",
      "workerd"
    ]
  }
}
